<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>SkyEditor.Core</AssemblyName>
    <PackageTargetFallback Condition=" '$(TargetFramework)' == 'netstandard1.6' ">$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ConsoleCommands\ConsoleCommand.cs" />
    <Compile Include="ConsoleCommands\ConsoleManager.cs" />
    <Compile Include="ConsoleCommands\ConsoleProvider.cs" />
    <Compile Include="ConsoleCommands\DummyConsoleProvider.cs" />
    <Compile Include="ConsoleCommands\IConsoleProvider.cs" />
    <Compile Include="CoreSkyEditorPlugin.cs" />
    <Compile Include="IOUIManager.cs" />
    <Compile Include="IO\IFileSaver.cs" />
    <Compile Include="IO\IIOProvider.cs" />
    <Compile Include="IO\INotifyModified.cs" />
    <Compile Include="IO\IOnDisk.cs" />
    <Compile Include="IO\ISavable.cs" />
    <Compile Include="ISettingsProvider.cs" />
    <Compile Include="PluginManager.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="Properties\Resources.Designer.cs" />
    <Compile Include="SettingsProvider.cs" />
    <Compile Include="Settings\ISettingsProviderExtensions.cs" />
    <Compile Include="Settings\SettingNames.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SettingNames.resx</DependentUpon>
    </Compile>
    <Compile Include="SkyEditorPlugin.cs" />
    <Compile Include="TestComponents\MemoryConsoleProvider.cs" />
    <Compile Include="TestComponents\MemoryIOProvider.cs" />
    <Compile Include="TypeRegisteredEventArgs.cs" />
    <Compile Include="UI\FileViewModel.cs" />
    <Compile Include="UI\GenericViewModel.cs" />
    <Compile Include="UI\RelayCommand.cs" />
    <Compile Include="Utilities\INamed.cs" />
    <Compile Include="Utilities\IReportProgress.cs" />
    <Compile Include="Utilities\Json.cs" />
    <Compile Include="Utilities\ParsedAssemblyQualifiedName.cs" />
    <Compile Include="Utilities\ProgressReportedEventArgs.cs" />
    <Compile Include="Utilities\ReflectionHelpers.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\Resources.resx" />
    <EmbeddedResource Include="Settings\SettingNames.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SettingNames.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <None Include="UI\AnchorableViewModel.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json">
      <Version>9.0.1</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>$(DefineConstants);RELEASE</DefineConstants>
  </PropertyGroup>
</Project>